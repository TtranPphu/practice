{
  "name": "practice-compose",
  "initializeCommand": "sh .devcontainer/initialize.sh",
  "postCreateCommand": "sh .devcontainer/post-create.sh",
  "dockerComposeFile": "docker-compose.yml",
  "service": "devcontainer",
  "workspaceFolder": "/workspaces/practice",
  "customizations": {
    "vscode": {
      "settings": {
        // https://github.com/ryanoasis/nerd-fonts/releases/download/v3.3.0/CascadiaCode.zip
        "editor.fontFamily": "Consolas, 'Courier New', monospace, 'CaskaydiaCove Nerd Font'",
        "terminal.integrated.fontFamily": "CaskaydiaCove Nerd Font",
        "workbench.colorTheme": "Default Dark Modern"
      },
      "extensions": [
        "albymor.increment-selection",
        "chekweitan.compare-view",
        "cweijan.vscode-mysql-client2",
        "eriklynd.json-tools",
        "esbenp.prettier-vscode",
        "github.copilot",
        "github.vscode-pull-request-github",
        "mechatroner.rainbow-csv",
        "medo64.render-crlf",
        "ms-azuretools.vscode-docker",
        "ms-python.black-formatter",
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-toolsai.jupyter",
        "ms-vscode.cmake-tools",
        "ms-vscode.cpptools",
        "rioj7.command-variable",
        "rust-lang.rust-analyzer",
        "tamasfe.even-better-toml",
        "usernamehw.errorlens",
        "vadimcn.vscode-lldb"
      ]
    }
  }
}
